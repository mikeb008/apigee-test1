<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="InternalServerError">
    <DisplayName>InternalServerError</DisplayName>
    <Properties/>
    <FaultResponse>
        <Set>
            <Headers/>
            <StatusCode>500</StatusCode>
            <ReasonPhrase>Internal Server Error</ReasonPhrase>
            <Payload contentType="application/json">{response.content}</Payload>
        </Set>
    </FaultResponse>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>